#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {

    int candle,i,j;
    int count=1;
    scanf("%d",&candle);
          for(i=1;i<=candle;i++)
          {
              for(j=2;j<=i/2;j++)
              {
                  if(i%j==0)
                  {
                      count++;
                      break;
                  }
              }
          }
    printf("%d",count);
    

    return 0;
}

