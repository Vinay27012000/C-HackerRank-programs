#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
long int n,num;
    int sum=0,a[20],count=0;
    scanf("%ld",&n);
    num=n;
    for(int i=0;i<20;i++)
    {if(num>0)
    {  a[i]=num%10;
        num=num/10;
    count++;}
     else
         break;
    }
        sum=a[0]+a[count-1];
    printf("%d",sum);
    return 0;
}
