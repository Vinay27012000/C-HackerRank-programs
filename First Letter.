#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
char str[20],s[20],l=0;
    int len,j=0;
    gets(str);
    
len=sizeof(str);
    for(int i=0;i<len;i++)
        if(str[i])
            ++l;
    s[j]=str[0];
    for(int i=1;i<=l;i++){
        if(str[i]==' ')
        {
            s[++j]=str[i+1];
            
        }
    }
   for(int i=0;i<=j;i++)
       printf("%c",s[i]);
    return 0;
}
